{
  "private": true,
  "name": "semirara",
  "version": "0.0.1",
  "description": "wiki",
  "main": "run-server.js",
  "engines": {
    "node": "12.14.0",
    "npm": "6.x"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "node run-server.js",
    "start:dev": "DEBUG=semirara*,koa* node-dev run-server.js",
    "test": "npm run mocha",
    "mocha": "mocha 'src/{*,**/*}/test/*.js' --compilers js:babel-register -r babel-polyfill",
    "eslint": "eslint 'src/*/{*,**/*}.{js,jsx}'",
    "webpack": "webpack --mode production",
    "build": "npm run babel && npm run webpack && npm run stylus",
    "stylus": "stylus -I node_modules/nib/lib -I node_modules/highlight.js/styles --include-css src/client/styl/index.styl -o public/dist/index.css",
    "babel": "babel src/ --out-dir dist/ --source-maps inline",
    "watch": "parallelshell 'chokidar \"src/**.{js,jsx}\" -c \"npm run test\"' 'npm run babel -- --watch' 'npm run stylus -- --watch'",
    "clean": "rm -rf 'dist/*' 'public/dist/*'"
  },
  "keywords": [
    "wiki",
    "outline"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.12.1",
    "axios": "^0.11.0",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.3",
    "clone": "^1.0.2",
    "cookie": "^0.3.1",
    "core-js": "^3.6.5",
    "date-utils": "^1.2.21",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "feed": "^0.3.0",
    "has-dom": "^1.0.0",
    "highlight.js": "^9.3.0",
    "koa": "^2.0.0",
    "koa-66": "^0.8.4",
    "koa-convert": "^1.2.0",
    "koa-logger": "^2.0.0",
    "koa-react-view": "^2.0.0",
    "koa-static": "^2.0.0",
    "lodash.uniq": "^4.2.1",
    "md5": "^2.0.0",
    "memjs": "^0.10.0",
    "mongoose": "^5.10.7",
    "mongoose-auto-increment": "^5.0.1",
    "nib": "^1.1.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "redux": "^3.4.0",
    "regenerator-runtime": "^0.13.7",
    "shortid": "^2.2.6",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6",
    "socket.io-request": "^0.1.0",
    "stylus": "^0.54.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "mocha": "^2.4.5",
    "node-dev": "^3.1.0",
    "parallelshell": "^2.0.0",
    "supertest": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shokai/semirara.git"
  },
  "bugs": {
    "url": "https://github.com/shokai/semirara/issues"
  },
  "homepage": "https://github.com/shokai/semirara#readme"
}
