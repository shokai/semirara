{
  "name": "semirara",
  "version": "0.0.1",
  "description": "wiki",
  "main": "server/run.js",
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "scripts": {
    "prestart": "npm run build && npm run browserify",
    "start": "node server/run.js",
    "start:dev": "DEBUG=semirara*,koa* node-dev server/run.js",
    "test": "npm run eslint && npm run mocha",
    "mocha": "mocha */test/*.es6 --compilers js:babel-register",
    "eslint": "eslint server/src/{*,**/*}.es6 client/{*,**/*}.{es6,jsx}",
    "browserify": "browserify --verbose --debug --extension=.jsx --extension=.es6 -t [ babelify --presets [es2015 react] ] client/index.es6 | uglifyjs -o public/dist/index.js",
    "watchify": "watchify --verbose --debug --extension=.jsx --extension=.es6 -t [ babelify --presets [es2015 react] ] client/index.es6 -o public/dist/index.js",
    "build": "babel server/src/ --out-dir server/dist/ --source-maps inline",
    "watch": "parallelshell 'chokidar \"*/**.{es6,jsx}\" -c \"npm run test\"' 'npm run build -- --watch' 'npm run watchify'",
    "clean": "rm -rf server/dist public/dist/*"
  },
  "keywords": [
    "wiki",
    "outline"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.5.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "koa": "^2.0.0-alpha.3",
    "koa-66": "^0.8.4",
    "koa-convert": "^1.2.0",
    "koa-jade": "^2.1.0",
    "koa-logger": "^2.0.0",
    "koa-static": "^2.0.0",
    "md5": "^2.0.0",
    "memjs": "^0.9.0",
    "mongoose": "^4.4.5",
    "mongoose-auto-increment": "^5.0.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "reactify": "^1.1.1",
    "redux": "^3.3.1",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "socket.io-request": "^0.0.4",
    "uglify-js": "^2.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^2.2.0",
    "eslint-plugin-if-in-test": "^0.2.0",
    "eslint-plugin-react": "^4.1.0",
    "mocha": "^2.4.5",
    "node-dev": "^3.1.0",
    "parallelshell": "^2.0.0",
    "redux-devtools": "^3.1.1",
    "watchify": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shokai/semirara.git"
  },
  "bugs": {
    "url": "https://github.com/shokai/semirara/issues"
  },
  "homepage": "https://github.com/shokai/semirara#readme"
}
